import java.util.Scanner;
public class starz{
  public static void main(String[]args){
    Scanner sc= new scanner(System.in);
    int n=sc.nextInt();
    for(int i=1;i<=n;i++){
      for(int j=1;j<=n;j++){
       System.out.println(" ");
      }
      for (int j=1;j<=i,j++){
        System.out.println("*");
      }
      System.out.println();
    }
  }
}